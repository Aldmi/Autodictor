	  <DataBits>8</DataBits>
	  <StopBits>1</StopBits>
	  <Parity>None</Parity>  
	  <DtrEnable>False</DtrEnable> 
	  <RtsEnable>False</RtsEnable> 
    </Serial>
  </SerialPorts>

  


	


